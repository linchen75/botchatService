@model IEnumerable<Me.WLF.Model.ConstMessage>

@{
    ViewBag.Title = "Const Messages";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@section NavConstMessages
{
class="active"
}
<table class="table table-striped table-bordered table-hover">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Format)</th>
        <th>@Html.DisplayNameFor(model => model.Content)</th>
        <th>@Html.DisplayNameFor(model => model.Language)</th>
        <th>Management</th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.Format</td>
            <td>
                @t.Content
            </td>
            <td>@t.Language</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { Id = @t.Id }) | 
                @Html.ActionLink("Del", "Del", new { Id = @t.Id })</td>
        </tr>
    }
</table>
