@model IEnumerable<Me.WLF.Model.User>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@section NavUser
{
class="active"
}
<h1>Users List</h1>

<table class="table table-striped table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Degree)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Language)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegisterTime)
        </th>
        <th>@Html.DisplayNameFor(model => model.Status)</th>
        <th>Management</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td class="span3">
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td class="span1">
                @Html.DisplayFor(modelItem => item.ClientId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Degree)
            </td>
            <td>@Html.DisplayFor(modelItem => item.Language)</td>
            <td>@Html.DisplayFor(modelItem => item.RegisterTime)</td>
            <td>@Html.DisplayFor(modelItem => item.Status)</td>
            <td class="span1">
                @Html.ActionLink("Messages", "Messages", new { userId = item.UserName })
                @Html.ActionLink("Delete", "Del", new { id = item.Id })
            </td>
        </tr>
    }

</table>
