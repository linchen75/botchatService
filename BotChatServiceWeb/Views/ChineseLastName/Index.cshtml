@model List<Me.WLF.Model.ChineseLastName>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@section NavChineseName
{
class="active"
}

<h1>Valid Chinese LastName</h1>

@Html.ActionLink("Create", "Add", null, new { @class = "btn btn-primary" })

<table class="table table-striped table-bordered table-hover">
    <tr>
        <th class="span10">Names</th>
        <th>Management</th>
    </tr>
    @if (Model == null || Model.Count > 0)
    {
        foreach (var t in Model)
        {
        <tr>
            <td>@t.LastName</td>
            <td class="span1">
                @Html.ActionLink("Del", "Del", new { Id = @t.Id })
            </td>
        </tr>
        }
    }
    else
    {
        <tr>
            <td colspan="3">No Record</td>
        </tr>
    }
</table>
