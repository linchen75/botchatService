@model List<Me.WLF.Model.ChineseLastName>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@Html.ActionLink("Add", "Add")

<table class="table table-striped table-bordered table-hover">
    <tr>
        <th>Names</th>
        <th>Management</th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.LastName</td>
            <td class="span1">
                @Html.ActionLink("Del", "Del", new { Id = @t.Id })
            </td>
        </tr>
    }
</table>
